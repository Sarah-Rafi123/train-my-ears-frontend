{
  "expo": {
    "name": "Train My Ear",
    "slug": "frontend_trainmyears",
    "version": "24.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "frontendtrainmyears",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.brianandrews.trainmyears",
      "icon": "./assets/images/icon.png",
      "buildNumber": "17",
      "infoPlist": {
        "CFBundleDisplayName": "Train My Ear",
        "CFBundleName": "Train My Ear",
        "CFBundleShortVersionString": "4.0.0",
        "UIBackgroundModes": [
          "audio"
        ],
        "LSRequiresIPhoneOS": false,
        "UIRequiresFullScreen": false,
        "UIInterfaceOrientation": "UIInterfaceOrientationPortrait",
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown"
        ]
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.brianandrews.trainmyears",
      "versionCode": 26,
      "screenOrientation": "portrait",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "trainmyears",
              "host": "oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
       "projectId": "6a7ba359-637b-4c8f-a89e-e5c56f873354"
      }
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-av",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "jsEngine": "hermes",
    "__internal__": {
      "isDebug": false
    }
  }
}
