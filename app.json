{
  "expo": {
    "name": "Train My Ear",
    "slug": "frontend_trainmyears",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "frontendtrainmyears",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brianandrews.trainmyears",
      "icon": "./assets/images/icon.png",
      "buildNumber": "16",
      "infoPlist": {
        "CFBundleDisplayName": "Train My Ear",
        "CFBundleName": "Train My Ear",
        "CFBundleShortVersionString": "4.0.0"
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.brianandrews.trainmyears",
      "versionCode": 16,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "frontendtrainmyears",
              "host": "oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "6a7ba359-637b-4c8f-a89e-e5c56f873354"
      }
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "brianandrews"
  }
}
