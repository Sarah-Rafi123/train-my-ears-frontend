{
  "expo": {
    "name": "Train My Ear",
    "slug": "frontend_trainmyears",
    "version": "13.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "frontendtrainmyears",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brianandrews.trainmyears",
      "icon": "./assets/images/icon.png",
      "buildNumber": "16",
      "infoPlist": {
        "CFBundleDisplayName": "Train My Ear",
        "CFBundleName": "Train My Ear",
        "CFBundleShortVersionString": "4.0.0",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.brianandrews.trainmyears",
      "versionCode": 16,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "frontendtrainmyears",
              "host": "oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
    "eas": {
        "projectId": "8463aea5-6e8e-44f7-b6cc-5291aa787d4d"
      }
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "jsEngine": "hermes",
    "__internal__": {
      "isDebug": false
    }
  }
}
