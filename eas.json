{
  "cli": {
    "version": ">= 8.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "EXPO_NO_DEV_MENU": "1",
        "EXPO_NO_DEV_LAUNCHER": "1"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "bob@experientialretreats.com",
        "ascAppId": "6748353964",
        "appleTeamId": "Q2Z3X4Y5Z6"
      }
    }
  }
}
